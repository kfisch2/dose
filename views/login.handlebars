<form class="login-form">
    <div>
        <label for="login-username">username:</label>
        <input type="text" id="login-username" placeholder="username" />
    </div>
    <div>
        <label for="login-pw">password:</label>
        <input type="text" id="login-pw" placeholder="password" />
    </div>
    <div>
        <button type="submit">Login</button>
    </div>
</form>

<form class="register-form">
    <div>
        <label for="register-username">username:</label>
        <input type="text" id="register-username" placeholder="username (Required)" />
    </div>
    <div>
        <label for="register-email">email:</label>
        <input type="text" id="register-email" placeholder="email (Registered)"/>
    </div>
    <div>
        <label for="register-number">phone number:</label>
        <input type="text" id="register-number" placeholder="phone number (Optional)" />
    </div>
    <div>
        <label for="register-pw">password:</label>
        <input type="text" id="register-pw" placeholder="password (Required)" />
    </div>
    <div>
        <button type="submit">Register</button>
    </div>
</form>

<script src="/public/javascript/login.js"
